def ordenamiento_por_insercion(lista):
    n = len(lista)
    for i in range(1, n):
        clave = lista[i]
        j = i - 1
        
        # Mover los elementos de lista[0..i-1] que son mayores que la clave
        # a una posiciÃ³n adelante de su posiciÃ³n actual
        while j >= 0 and lista[j] < clave:
            lista[j + 1] = lista[j]
            j -= 1 
        
        # Colocar la clave en su posiciÃ³n correcta
        lista[j + 1] = clave
    
    return lista

# Ejemplo de uso
numeros = [12, 25, 14, 23, 1]
print("Lista original:", numeros)
print("Lista ordenada:", ordenamiento_por_insercion(numeros))
